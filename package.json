{
  "name": "galaxybot",
  "productName": "Galaxy Bot",
  "description": "Powerful cloud-oriented client allow you to connect to various applications via secure (SSH) tunnel",
  "version": "0.0.1",
  "author": "Sanket Bajoria <bajoriasanket@gmail.com>",
  "homepage": "http://sanketbajoria.com",
  "license": "MIT",
  "main": "dist/background.js",
  "build": {
    "appId": "com.connectbot",
    "files": [
      "dist/**/*",
      "node_modules/**/*",
      "package.json"
    ],
    "publish": null
  },
  "scripts": {
    "postinstall": "install-app-deps",
    "build": "gulp build",
    "prerelease": "gulp build --env=production",
    "release": "build",
    "start": "gulp start",
    "pretest": "gulp build-unit --env=test",
    "test": "electron-mocha dist/specs.js.autogenerated --renderer --require source-map-support/register",
    "coverage": "npm test -- -R scripts/istanbul-reporter",
    "pree2e": "gulp build-e2e --env=test",
    "e2e": "mocha dist/e2e.js.autogenerated --require source-map-support/register"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "angular": "^1.6.5",
    "angular-animate": "^1.6.6",
    "angular-route": "^1.6.6",
    "angular-toastr": "^2.1.1",
    "angular-ui-bootstrap": "^2.5.0",
    "aws-sdk": "^2.100.0",
    "bootstrap": "^3.3.7",
    "bourbon": "^4.3.4",
    "cli-color": "^1.2.0",
    "clone": "^2.1.1",
    "debug": "^3.0.1",
    "draggabilly": "^2.1.1",
    "font-awesome": "^4.7.0",
    "fs-extra": "^4.0.1",
    "fs-jetpack": "^1.0.0",
    "galaxyfauxton": "https://github.com/sanketbajoria/couchdb-fauxton/raw/master/fauxton-1.1.13.tgz",
    "get-port": "^3.1.0",
    "gulp-autoprefixer": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "json-query": "^2.2.2",
    "lokijs": "^1.5.0",
    "q": "^1.5.0",
    "read": "^1.0.7",
    "request": "^2.81.0",
    "socksv5": "0.0.6",
    "ssh2": "^0.5.5",
    "xregexp": "^3.2.0",
    "xterm": "^2.9.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "electron": "^1.6.11",
    "electron-builder": "^19.16.0",
    "electron-mocha": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-batch": "^1.0.5",
    "gulp-plumber": "^1.1.0",
    "gulp-sass": "^3.1.0",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^4.3.11",
    "istanbul": "^0.4.5",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "rollup": "^0.45.2",
    "rollup-plugin-istanbul": "^1.1.0",
    "source-map-support": "^0.4.15",
    "spectron": "^3.7.2"
  }
}
